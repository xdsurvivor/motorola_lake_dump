<!-- This file is used to define the mappings between lower-level system
     user and group IDs and the higher-level permission names managed
     by the platform.

     Be VERY careful when editing this file!  Mistakes made here can open
     big security holes.
-->
<permissions>

    <!-- These are the standard packages that are white-listed to always have internet
         access while in power save mode, even if they aren't in the foreground. -->
    <allow-in-power-save package="com.zui.sdac" />
    <allow-in-power-save package="com.zui.auservice" />
    <allow-in-power-save package="com.zui.xlog" />
    <allow-in-power-save package="com.zui.bugtogo" />

    <!-- shenqi-framework, wangwq14, XUICR-78, begin
         The following package can be install in background in silence.
         'name' is the permissions can be automatic grant to the package signed by 'signature' -->
    <!-- SHAREit app and CloudService -->
    <allow-permission name="android.permission.INSTALL_PACKAGES" signature="" />
    <!-- LenovoStore app require INSTALL_PACKAGES,DELETE_PACKAGES,MOVE_PACKAGES and PACKAGE_USAGE_STATS,split by :-->
    <allow-permission name="android.permission.INSTALL_PACKAGES:android.permission.DELETE_PACKAGES:android.permission.MOVE_PACKAGES:android.permission.PACKAGE_USAGE_STATS" signature=""/>
    <!-- GameCenter app -->
    <allow-permission name="android.permission.INSTALL_PACKAGES:android.permission.DELETE_PACKAGES:android.permission.MOVE_PACKAGES:android.permission.PACKAGE_USAGE_STATS" signature=""/>
    <!-- SHARECloneit app "com.lenovo.anyshare.cloneit"-->
    <allow-permission name="android.permission.INSTALL_PACKAGES" signature="" />
    <!-- PhoneSky app -->
    <allow-permission name="android.permission.ACCESS_DOWNLOAD_MANAGER:android.permission.ACCESS_DOWNLOAD_MANAGER_ADVANCED:android.permission.ALLOCATE_AGGRESSIVE:android.permission.BATTERY_STATS:android.permission.BACKUP:android.permission.BIND_APPWIDGET:android.permission.CALL_PRIVILEGED:android.permission.CAPTURE_AUDIO_HOTWORD:android.permission.CHANGE_COMPONENT_ENABLED_STATE:android.permission.CHANGE_CONFIGURATION:android.permission.CHANGE_DEVICE_IDLE_TEMP_WHITELIST:android.permission.CHANGE_NETWORK_STATE:android.permission.DELETE_PACKAGES:android.permission.CLEAR_APP_CACHE:android.permission.DUMP:android.permission.CONNECTIVITY_INTERNAL:android.permission.FORCE_STOP_PACKAGES:android.permission.GET_ACCOUNTS_PRIVILEGED:android.permission.GET_APP_OPS_STATS:android.permission.GET_PACKAGE_IMPORTANCE:android.permission.GET_ACCOUNTS_PRIVILEGED:android.permission.GLOBAL_SEARCH:android.permission.INSTALL_GRANT_RUNTIME_PERMISSIONS:android.permission.INSTALL_PACKAGES:android.permission.INTERACT_ACROSS_USERS:android.permission.MANAGE_USERS:android.permission.MANAGE_VOICE_KEYPHRASES:android.permission.MEDIA_CONTENT_CONTROL:android.permission.MODIFY_NETWORK_ACCOUNTING:android.permission.PACKAGE_USAGE_STATS:android.permission.PACKAGE_VERIFICATION_AGENT:android.permission.REAL_GET_TASKS:android.permission.READ_PRIVILEGED_PHONE_STATE:android.permission.REVOKE_RUNTIME_PERMISSIONS:android.permission.SEND_SMS_NO_CONFIRMATION:android.permission.SET_PREFERRED_APPLICATIONS:android.permission.STATUS_BAR:android.permission.STOP_APP_SWITCHES:android.permission.UPDATE_CONFIG:android.permission.UPDATE_DEVICE_STATS:android.permission.WRITE_SECURE_SETTINGS:android.permission.WRITE_SETTINGS:com.android.voicemail.permission.READ_VOICEMAIL" signature="" />
    <!-- UserCenter app require INSTALL_PACKAGES,DELETE_PACKAGES,MOVE_PACKAGES and PACKAGE_USAGE_STATS,split by :-->
    <allow-permission name="android.permission.INSTALL_PACKAGES:android.permission.DELETE_PACKAGES:android.permission.MOVE_PACKAGES:android.permission.PACKAGE_USAGE_STATS" signature="" />

    <!-- shenqi-framework, wangwq14, XUICR-78, end -->

    <!-- ZUIO-12206 grant permission to LenovoBlockChain-->
    <allow-permission name="android.permission.PACKAGE_USAGE_STATS" signature="" />


    <!-- ZUIO-18177 grant permission to LenovoVoice-->
    <allow-permission name="android.permission.PEERS_MAC_ADDRESS" signature="" />

</permissions>
